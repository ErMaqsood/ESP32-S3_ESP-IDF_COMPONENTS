<codeline><highlight class="normal"><sp/><sp/><sp/><sp/><ref refid="group__drivers_1gafad46afc4709cc67901aef0e43fbedae" kindref="member">as3935_monitor_init</ref>(i2c0_bus_hdl,<sp/>&amp;dev_cfg,<sp/>&amp;monitor_hdl);</highlight></codeline>
<codeline><highlight class="normal"><sp/><sp/><sp/><sp/></highlight><highlight class="keywordflow">if</highlight><highlight class="normal"><sp/>(monitor_hdl<sp/>==<sp/>NULL)<sp/>{</highlight></codeline>
<codeline><highlight class="normal"><sp/><sp/><sp/><sp/><sp/><sp/><sp/><sp/>ESP_LOGE(APP_TAG,<sp/></highlight><highlight class="stringliteral">&quot;as3935_monitor_init<sp/>handle<sp/>init<sp/>failed&quot;</highlight><highlight class="normal">);</highlight></codeline>
<codeline><highlight class="normal"><sp/><sp/><sp/><sp/><sp/><sp/><sp/><sp/>assert(<ref refid="group__drivers_1gafad46afc4709cc67901aef0e43fbedae" kindref="member">as3935_monitor_init</ref>);</highlight></codeline>
